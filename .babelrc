{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "inline-import",
      {
        "extensions": [".css", ".less"]
      }
    ],
    "styled-jsx/babel",
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-function-bind",
    "transform-async-generator-functions"
  ]
}
